<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:nxe="http://www.roughseas.ca/xmleditor/1.0.0"
	xmlns:map="http://www.ndg.com/map" xmlns:mp="http://www.ndg.com/multiplayer/base" xmlns:mps="http://www.ndg.com/multiplayer/sessionbase"
	xmlns:momimecommon="http://momime/common/database/v0_9_4" xmlns:momimenewgame="http://momime/common/database/newgame/v0_9_4"
	xmlns:momimeclient="http://momime/client/database/v0_9_4" xmlns:momimemsgs="http://momime/common/messages/v0_9_5"
	xmlns:momimectos="http://momime/common/messages/clienttoserver/v0_9_5" xmlns:momimestoc="http://momime/common/messages/servertoclient/v0_9_5"
	xmlns:momimedummy="http://momime/dummycommon/database/v0_9_4" targetNamespace="http://momime/dummycommon/database/v0_9_4">
	
	<xsd:import namespace="http://www.ndg.com/map"/>
	<xsd:import namespace="http://www.ndg.com/multiplayer/base"/>
	<xsd:import namespace="http://www.ndg.com/multiplayer/sessionbase"/>
	<xsd:import namespace="http://momime/common/database/newgame/v0_9_4"/>
	<xsd:import namespace="http://momime/common/database/v0_9_4"/>

	<!-- Following 4 are only here because they are present in MoMIMECommon's episode file and so we have to declare them even if we don't use them -->

	<xsd:import namespace="http://momime/client/database/v0_9_4"/>
	<xsd:import namespace="http://momime/common/messages/v0_9_5"/>
	<xsd:import namespace="http://momime/common/messages/clienttoserver/v0_9_5"/>
	<xsd:import namespace="http://momime/common/messages/servertoclient/v0_9_5"/>

	<!-- Dummy XSD containing at least one reference to each of the XSDs that CommonXsdResourceResovler needs
			to resolve, just enough to prove that the imports all get resolved correctly -->
	
	<xsd:element name="dummyDatabase">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="raceID" type="momimecommon:raceID"/>
				<xsd:element name="percentage" type="momimenewgame:percentage"/>
				<xsd:element name="direction" type="map:mapDirection"/>
				<xsd:element name="sessionID" type="mps:sessionID"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
</xsd:schema>

