				<xsd:element minOccurs="0" maxOccurs="1" name="virtualKey" type="momimelangs:virtualKey" nxe:label="Virtual Key" nxe:gridwidth="70" />
			<!-- </xsd:choice> -->
		</xsd:sequence>
		<xsd:attribute name="language" type="momimecommon:language" use="required" nxe:label="Code" nxe:gridwidth="150" />
	</xsd:complexType>
	
