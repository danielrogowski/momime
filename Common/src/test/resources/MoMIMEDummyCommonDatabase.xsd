<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:nxe="http://www.roughseas.ca/xmleditor/1.0.0"
	xmlns:map="http://www.ndg.com/map" xmlns:mp="http://www.ndg.com/multiplayer/base" xmlns:mps="http://www.ndg.com/multiplayer/sessionbase"
	xmlns:momimecommon="http://momime/common/database/v0_9_4" xmlns:momimenewgame="http://momime/common/database/newgame/v0_9_4"
	xmlns:momimedummy="http://momime/dummycommon/database/v0_9_4" targetNamespace="http://momime/dummycommon/database/v0_9_4">
	
	<xsd:import namespace="http://www.ndg.com/map" schemaLocation="../../../../../NdgMap/src/main/resources/com.ndg.map/Map.xsd" />
	<xsd:import namespace="http://www.ndg.com/multiplayer/base" schemaLocation="../../../../../NdgMultiplayerBase/src/main/resources/com.ndg.multiplayer.messages/Multiplayer.xsd" />
	<xsd:import namespace="http://www.ndg.com/multiplayer/sessionbase" schemaLocation="../../../../../NdgMultiplayerSessionBase/src/main/resources/com.ndg.multiplayer.messages/MultiplayerSession.xsd" />
	<xsd:import namespace="http://momime/common/database/newgame/v0_9_4" schemaLocation="../../../main/resources/momime.common.database/MoMIMENewGameDatabase.xsd" />
	<xsd:import namespace="http://momime/common/database/v0_9_4" schemaLocation="../../../main/resources/momime.common.database/MoMIMECommonDatabase.xsd" />

	<!-- Dummy XSD containing at least one reference to each of the XSDs that CommonXsdResourceResovler needs
			to resolve, just enough to prove that the imports all get resolved correctly -->
	
	<xsd:element name="dummyDatabase">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="raceID" type="momimecommon:raceID"/>
				<xsd:element name="percentage" type="momimenewgame:percentage"/>
				<xsd:element name="direction" type="map:mapDirection"/>
				<xsd:element name="sessionID" type="mps:sessionID"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
</xsd:schema>

