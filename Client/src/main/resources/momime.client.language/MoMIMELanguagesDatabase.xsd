<!-- Complex types -->
	
	<xsd:complexType name="languageOption" nxe:singular="Language option" nxe:plural="Language options">
        <xsd:annotation>
        	<xsd:documentation>Different languages supported by MoM IME.</xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="1" maxOccurs="1" name="languageDescription" type="momimecommon:description" nxe:label="Description" nxe:gridwidth="300" />
		</xsd:sequence>
		<xsd:attribute name="language" type="momimecommon:language" use="required" nxe:label="Code" nxe:gridwidth="150" />
	</xsd:complexType>	

	<xsd:complexType name="knownServer" nxe:singular="Known Server" nxe:plural="Known Servers">
        <xsd:annotation>
        	<xsd:documentation>Defines known MoM IME servers to suggest the client connects to.</xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" maxOccurs="unbounded" name="knownServerDescription" type="momimecommon:languageText" nxe:label="Description" nxe:gridwidth="150" />
			<xsd:element minOccurs="1" maxOccurs="1" name="knownServerIP" type="momimelangs:ipAddress" nxe:label="IP Address" nxe:gridwidth="200" />
		</xsd:sequence>
		<xsd:attribute name="knownServerID" type="momimelangs:knownServerID" use="required" nxe:label="Server ID" nxe:gridwidth="100" />
	</xsd:complexType>

	<xsd:complexType name="shortcutKey" nxe:singular="Shortcut Key" nxe:plural="Shortcut Keys">
		<xsd:sequence>
			<xsd:element minOccurs="0" maxOccurs="unbounded" name="keyLang" type="momimelangs:shortcutKeyLang" />
		</xsd:sequence>
		<xsd:attribute name="shortcut" type="momimelangs:shortcut" use="required" nxe:label="Shortcut" nxe:gridwidth="300" />
	</xsd:complexType>

	<xsd:complexType name="shortcutKeyLang" nxe:singular="Shortcut Key" nxe:plural="Shortcut Keys">
		<xsd:sequence>
			<xsd:element minOccurs="0" maxOccurs="1" name="shift" type="xsd:boolean" nxe:label="Shift?" nxe:gridwidth="50" />
			<xsd:element minOccurs="0" maxOccurs="1" name="alt" type="xsd:boolean" nxe:label="Alt?" nxe:gridwidth="50" />
			<xsd:element minOccurs="0" maxOccurs="1" name="ctrl" type="xsd:boolean" nxe:label="Ctrl?" nxe:gridwidth="50" />
			<!-- <xsd:choice minOccurs="1" maxOccurs="1" > -->
				<xsd:element minOccurs="0" maxOccurs="1" name="normalKey" type="momimelangs:normalKey" nxe:label="Key" nxe:gridwidth="50" />
				<xsd:element minOccurs="0" maxOccurs="1" name="virtualKey" type="momimelangs:virtualKey" nxe:label="Virtual Key" nxe:gridwidth="70" />
			<!-- </xsd:choice> -->
		</xsd:sequence>
		<xsd:attribute name="language" type="momimecommon:language" use="required" nxe:label="Code" nxe:gridwidth="150" />
	</xsd:complexType>
	
